<dataformat>
  <name>reads_fastq</name>
  <description>reads in fastq format</description>
  <alias>fastq</alias>
  <prefix>reads</prefix>
  <samplemetadatakey>Reads</samplemetadatakey>
  <maxfilescount>2</maxfilescount>
  <checker>fr.ens.transcriptome.eoulsan.checkers.ReadsChecker</checker>
  <splitter>fr.ens.transcriptome.eoulsan.splitermergers.FastqSplitter</splitter>
  <merger>fr.ens.transcriptome.eoulsan.splitermergers.FastqMerger</merger>
  <extensions>
  	<extension default="true">.fq</extension>
  	<extension>.fastq</extension>
  </extensions>

  <toolshedgalaxy>
  	<extension>fq</extension>
  	<extension>fastq</extension>    	
  </toolshedgalaxy>
</dataformat>
