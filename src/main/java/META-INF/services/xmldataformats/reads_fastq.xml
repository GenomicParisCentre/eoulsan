<dataformat>
  <name>reads_fastq</name>
  <description>reads in fastq format</description>
  <alias>fastq</alias>
  <prefix>reads</prefix>
  <samplemetadatakey>Reads</samplemetadatakey>
  <maxfilescount>2</maxfilescount>
  <checker>fr.ens.biologie.genomique.eoulsan.checkers.ReadsChecker</checker>
  <splitter>fr.ens.biologie.genomique.eoulsan.splitermergers.FastqSplitter</splitter>
  <merger>fr.ens.biologie.genomique.eoulsan.splitermergers.FastqMerger</merger>
  <extensions>
    <extension default="true">.fq</extension>
    <extension>.fastq</extension>
  </extensions>
  <galaxy>
    <formatname>fq</formatname>
    <formatname>fastq</formatname>
  </galaxy>
</dataformat>
